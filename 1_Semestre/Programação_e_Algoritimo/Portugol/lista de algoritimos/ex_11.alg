algoritmo "semnome"
// Função :
// Autor :
// Data : 13/03/2024
// Seção de Declarações 
var
custo,venda,lucro:real
inicio
//entrada
escreval("digite o produto a preço de custo:")
leia(custo)
escreval("digite a porcentagem de lucro:")
leia(lucro)

//processamento
venda<-lucro/100+custo



//saida
escreval("lucro: R$",lucro, "  venda: R$",venda)




fimalgoritmo
